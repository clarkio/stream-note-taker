{
  "env": {
    "node": true,
    "es6": true,
    "mocha": true
  },
  "extends": "wesbos",
  "parserOptions": {},
  "rules": {
    "quotes": ["error", "single", { "allowTemplateLiterals": true }],
    "semi": ["error", "always"],
    "no-use-before-define": "off",
    "comma-dangle": ["error", "never"],
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "none",
        "singleQuote": true
      }
    ]
  }
}
